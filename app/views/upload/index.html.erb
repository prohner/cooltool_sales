<h1>Upload#upload</h1>
<p>Find me in app/views/upload/upload.html.erb</p>

<%= form_tag({:action => :upload}, multipart: true) do %>
  <%= file_field_tag :file %>
  <%= submit_tag( "Upload" ) %>
<% end %>


<%= render partial: "upload_files/upload_files_collection", locals: {apps: @apps, upload_files: @upload_files} %>
